pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Develop') {
            steps {
                echo 'Develop the program'
            }
        }
        stage('Test') {
            steps {
                echo 'Test the program'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy the program'
            }
        }
    }
}
